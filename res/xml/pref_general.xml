<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

     <ListPreference
        android:entries="@array/pref_list_icon"
        android:entryValues="@array/pref_list_icon_values"
        android:defaultValue="0"
        android:key="icon"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_icon" />
        
    <ListPreference
        android:entries="@array/pref_list_position"
        android:entryValues="@array/pref_list_position_values"
        android:defaultValue="1"
        android:key="iconposition"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_position" />
   


</PreferenceScreen>
